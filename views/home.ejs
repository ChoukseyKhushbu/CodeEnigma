          <ul id="ques"> <!-- Href Calls -->
            <% questions.forEach(function(question){ %>
              <% if(user.type=="admin"){ %><a class="editButton" href="/update/<%= difficulty %>/<%= question.id %>" >[ EDIT ]</a><% } %>
              <li><a href="<%= difficulty %>/<%= question.id %>/<%= user.defaultLanguage %>">
